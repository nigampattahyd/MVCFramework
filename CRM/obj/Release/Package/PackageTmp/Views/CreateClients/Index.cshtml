<script>


    $("#btnSave").click(function (e) {
        // 
        var clientId = $('#txtClientId').val();

        var res1 = hasWhiteSpace(clientId);      
        if (res1 == false) {
            var pattern2 = /^[a-zA-Z].*/;
            var result2 = pattern2.test(clientId);
            if (result2 == true) {
