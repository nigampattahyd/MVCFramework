    function EditLead() {
        var leadid = $('#txtID').val();


        $.ajax({
            url: "../Leads/_EditLeadbyID",
            type: "GET",
            async: false,
            data: { LeadId: leadid },
            success: function (data) {

                if (data != null) {
                    $("#Getleads").empty().append(data);

                }
            },
            error: function () {

            }
        });
    }
    function deleteSelectedLead() {

        var leadid = $('#txtID').val();
        $.ajax({

            //url: "Leads/DeleteLeads",
            //url: "Leads/DeleteLeadsbyId",
            url: '@Url.Action("DeleteLeadsbyId", "Leads")',
            type: "POST",
            async: false,
            data: { leadid: leadid.toString() },
            success: function (data) {

                if (data == "True") {
                    $.growl.notice({ title: "Success", message: "Lead Deleted Successfully.", size: 'large' });
                    RedirectToindex();
                }
                else {
                    $.growl.notice({ title: "Fail", message: "Lead Not Deleted .", size: 'large' });
                }
            },
            error: function (request, status, error) {
            }
        });


