                <!-- Sidebar Menu -->
                <ul class="nav sidebar-menu">
                    <!--Dashboard-->
                    @{List<tbl_Menus> roleModel = (List<tbl_Menus>)Session["ssnDynamicMenus"];}
                    @foreach (var item in roleModel)
                    {
                        if (@item.ParentMenuId == 0)
                        {
                            var query = (from menu in roleModel where @item.MenuId == menu.ParentMenuId select menu).ToList();

                            if (query.Count == 0)
                            {
                                if (@item.MenuDescription == "Dashboard")
                                {

                                    <li id="Dashboard">
                                        <a href="@Url.Action(@item.ActionName,@item.ControllerName)">
                                            <i class="menu-icon fa fa-tachometer"></i>
                                            <span class="menu-text">@item.MenuDescription</span>
                                        </a>
                                    </li>
                                }
                                else if (@item.MenuDescription == "Ventures")
                                {

                                    <li id="Ventures">
                                        <a href="@Url.Action(@item.ActionName,@item.ControllerName)">
                                            <i class="menu-icon fa fa-users"></i>
                                            <span class="menu-text">@item.MenuDescription</span>
                                        </a>
                                    </li>
                                }
                                else if (@item.MenuDescription == "Mentors")
                                {

                                    <li id="Mentors">
                                        <a href="@Url.Action(@item.ActionName,@item.ControllerName)">
                                            <i class="menu-icon fa fa-user-times"></i>
                                            <span class="menu-text">@item.MenuDescription</span>
                                        </a>
                                    </li>
                                }
                                else if (@item.MenuDescription == "Activities")
                                {

                                    <li id="Activities">
                                        <a href="@Url.Action(@item.ActionName,@item.ControllerName)">
                                            <i class="menu-icon fa fa-line-chart "></i>
                                            <span class="menu-text">@item.MenuDescription</span>
                                        </a>
                                    </li>
                                }
                                else if (@item.MenuDescription == "Notifications")
                                {

                                    <li id="Notifications">
                                        <a href="@Url.Action(@item.ActionName,@item.ControllerName)">
                                            <i class="menu-icon fa fa-sliders"></i>
                                            <span class="menu-text">@item.MenuDescription</span>
                                        </a>
                                    </li>
                                }
                                else if (@item.MenuDescription == "Events")
                                {

                                    <li id="activemenu" class="activemenu">
                                        <a href="@Url.Action(@item.ActionName,@item.ControllerName)">
                                            <i class="menu-icon fa fa-calendar"></i>
                                            <span class="menu-text">@item.MenuDescription</span>
                                        </a>
                                    </li>
                                }
                                else
                                {

                                    <li id="home">
                                        <a href="@Url.Action(@item.ActionName,@item.ControllerName)">
                                            <i class="menu-icon fa fa-home"></i>
                                            <span class="menu-text">@item.MenuDescription</span>
                                        </a>
                                    </li>
                                }
                            }
                            else
                            {

                                <li id=@item.MenuDescription>
                                    <a href="#" class="menu-dropdown">
                                        @if (@item.MenuDescription == "Manage")
                                        {
                                            <i class="menu-icon  fa fa-group"></i>
                                            <span class="menu-text"> @item.MenuDescription </span>
                                        }
                                        else if (@item.MenuDescription == "Setup")
                                        {
                                            <i class="menu-icon  fa fa-cogs"></i>
                                            <span class="menu-text"> @item.MenuDescription </span>
                                        }
                                        else if (@item.MenuDescription == "Events")
                                        {
                                            <i class="menu-icon  fa fa-th"></i>
                                            <span class="menu-text"> @item.MenuDescription </span>
                                        }
                                        else
                                        {
                                            <i class="menu-icon fa fa-th"></i>
                                            <span class="menu-text"> @item.MenuDescription </span>
                                        }

                                        <i class="menu-expand"></i>
                                    </a>
                                    <ul class="submenu">
                                        @foreach (var item1 in query)
                                        {
                                            var query1 = (from menu in roleModel where @item1.MenuId == menu.ParentMenuId select menu).ToList();
                                            if (query1.Count == 0)
                                            {

                                                <li>
                                                    <a href="@Url.Action(@item1.ActionName, @item1.ControllerName)"><span class="mm-text" >@item1.MenuDescription</span></a>
                                                </li>
                                            }
                                            else
                                            {

                                                <li>
                                                    <a href="#" class="menu-dropdown"><i class="menu-icon fa fa-files-o"></i><span class="mm-text">@item1.MenuDescription</span></a>
                                                    <ul>
                                                        @foreach (var item2 in query1)
                                                        {
                                                            <li>
                                                                <a href="@Url.Action(@item2.ActionName, @item2.ControllerName)"><span class="mm-text">@item2.MenuDescription</span></a>
                                                            </li>
                                                        }
                                                    </ul>
                                                </li>
                                            }
                                        }
                                    </ul>
                                </li>
                            }
                        }
                    }

                </ul>
                <!-- /Sidebar Menu -->
            </div>
            <!-- /Page Sidebar -->

