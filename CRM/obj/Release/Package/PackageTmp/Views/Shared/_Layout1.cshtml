                        <a class="fullscreen" id="fullscreen-toggler" href="#">
                            <i class="glyphicon glyphicon-fullscreen"></i>
                        </a>
                    </div>
                    <!--Header Buttons End-->
                </div>

                <div class="page-body">
                    @RenderBody()
                </div>
                <div class="footer"><h5>Version 1.0.0.1 </h5></div>

            </div>

