+<script type="text/javascript">
+    $(document).ready(function () {
+        BindMentors();
+    });
+
+    function BindMentors() {
+        var responsiveHelper = undefined;
+        var breakpointDefinition = {
+            desktop: Infinity,
+            tablet: 1024,
+            tabletlandscape: 768,
+            phone: 480
