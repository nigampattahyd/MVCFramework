    <style>
        #loading {
            width: 100%;
            height: 100%;
            top: 0px;
            left: 200px;
            position: fixed;
            display: block;
            opacity: 0.7;
            background-color: #fff;
            z-index: 99;
            text-align: center;
        }

        #loading-image {
            position: absolute;
            top: 300px;
            left: 450px;
            z-index: 100;
        }

        .Overlay {
            display: block;
            position: fixed;
            top: 0px;
            bottom: 0px;
            left: 0px;
            right: 0px;
            overflow: hidden;
            padding: 0;
            margin: 0;
            background-color: #000;
            filter: alpha(opacity=50);
            opacity: 0.5;
            z-index: 1049;
        }

        .notification {
            right: 0;
            background-color: #fff;
            color: #fff;
            font-size: 19px;
            display: block;
        }

        @@media (min-width: 768px) {
            #main-navbar li a {
                line-height: 25px;
            }
        }

        .widget-notifications .notification-ago, .widget-notifications .notification-description, .widget-notifications .notification-title {
