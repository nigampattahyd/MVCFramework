@{
    ViewBag.Title = "DashboarHome";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}


<div class="widget">
    <div class="widget-header bordered-bottom bordered-themesecondary">
        <i class="widget-icon fa fa-tags themesecondary"></i>
        <span class="widget-caption themesecondary">Getting Started with Mentoring</span>
    </div><!--Widget Header-->
    <div class="widget-body  no-padding">
        <div class="tickets-container">
            <div class="row" style="margin-bottom:10px; border-bottom:1px solid #ccc; padding-bottom:15px;">
                <div class="col-sm-1 col-xs-3">
                    <img src="~/Images/icon-6.png">
                </div>
                <div class="col-sm-10 col-xs-9">
                    <h4>Ventures</h4>
                    <p>"Venture" for the entrepreneur being mentored.</p>
                </div>
            </div>
            <div class="row" style="margin-bottom:10px; border-bottom:1px solid #ccc; padding-bottom:15px;">
                <div class="col-sm-1 col-xs-3">
                    <img src="~/Images/icon-1.png">
                </div>
                <div class="col-sm-10 col-xs-9">
                    <h4>Mentors</h4>
                    <p>
                        "Mentors" are selected for their experience in areas relevant to the needs of entrepreneurs.
                    </p>

                </div>
            </div>
            <div class="row" style="margin-bottom:10px; border-bottom:0 solid #ccc; padding-bottom:15px;">
                <div class="col-sm-1 col-xs-3">
                    <img src="~/Images/icon-3.png">
                </div>
                <div class="col-sm-10 col-xs-9">
                    <h4>Users</h4>
                    <p>"Users" are dependent on Mentors.</p>
                </div>
            </div>
        </div>
    </div>


</div>
<div class="widget">
    <div class="widget-header bordered-bottom bordered-themesecondary">
        <i class="widget-icon fa fa-tags themesecondary"></i>
        <span class="widget-caption themesecondary">Easy Setup for Administrator</span>
    </div><!--Widget Header-->
    <div class="widget-body  no-padding">
        <div class="tickets-container">
            <div class="row" style="margin-bottom:10px; border-bottom:1px solid #ccc; padding-bottom:15px;">
                <div class="col-sm-1 col-xs-3">
                    <img src="~/Images/icon-2.png">
                </div>
                <div class="col-sm-10 col-xs-9">
                    <h4>Add User</h4>
                    <p>Adding new Users</p>
                </div>
            </div>
            <div class="row" style="margin-bottom:10px; border-bottom:0 solid #ccc; padding-bottom:15px;">
                <div class="col-sm-1 col-xs-3">
                    <img src="~/Images/icon-4.png">
                </div>
                <div class="col-sm-10 col-xs-9">
                    <h4>Customization</h4>
                    <p>Give your users,roles, permissions to hide and access the modules.</p>
                </div>
            </div>
        </div>
    </div>
</div>


<script type="text/javascript">
    $(document).ready(function () {
        $("#home").addClass("active");
    });
</script>












